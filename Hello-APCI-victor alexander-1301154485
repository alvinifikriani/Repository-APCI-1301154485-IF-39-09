Program Hello;
        Uses Crt;
        Begin
            ClrScr;
            Writeln(‘Hello World’); //untuk mengeluarkan string ‘Hello World’ di layar
        End
            Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301154485’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘alvini fikriani’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘if 39-09’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘victor alexander’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘1301154527’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 16’); //Ubah untuk menampilkan string ‘Modul 6
            End.
